def is_leap_year(year):
  """This function will return whether the year inputted is a leap year or not.
  Input: int, year
  Output: boolean, True if it is a leap year. False if it is not a leap year."""
  if (year%4==0 and year%100!=0) or (year%400==0):
    return True
  else:
    return False

year = int(input("Enter year: "))

print(is_leap_year(year)) 
